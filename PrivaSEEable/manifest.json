{
  	"name": "PrivaSeaable",
	"version": "1.0",
	"description": "Adds custom edits to text on screen to protect your privacy",
	"manifest_version": 2,
	"browser_action":
	{
			"default_popup": "UI.html",
			"default_icon":"PrivaSeeable2.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://*/*",
				"https://*/*"
			],
			"js":[
				"content.js"
			],
			"run_at":"document_end"
		}
	],

	"icons": {
		"48":"PrivaSeeable2.png"
	}
}
